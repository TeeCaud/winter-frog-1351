<h1>Plots Index</h1>
<% @plots.each do |plot| %>
  <div id="plot-<%= "#{plot.number}" %>">
    <p>Plot: <%= plot.number %> </p>
    <% plot.plant_plots.each do |plant_plot| %>
    <%= "#{plant_plot.plant.name}" %>
    <%= link_to "Remove #{plant_plot.plant.name}", plant_plot_path(plant_plot.id), method: :delete %>
  <% end %>
  <br><br>
</div>
<% end %>
